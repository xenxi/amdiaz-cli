{
  "name": "amdiaz",
  "version": "1.0.3-beta",
  "description": "The bio-cli package is a command line interface tool that displays a brief summary of a developer",
  "main": "./dist/src/bio.js",
  "type": "module",
  "bin": {
    "amdiaz": "./dist/src/bio.js"
  },
  "scripts": {
    "start": "node dist/src/bio.js",
    "build": "tsc",
    "build:watch": "tsc -w",
    "dev": "npm run build && npm run start",
    "dev:watch": "concurrently \"npm run build:watch\" \"npx nodemon --watch ./dist/src/bio.js --exec node ./dist/src/bio.js\"",
    "pub": "npm run build && npm version minor & npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xenxi/bio-cli.git"
  },
  "keywords": [
    "amdiaz",
    "resume",
    "about"
  ],
  "author": "amdiaz (https://antoniomdm.dev/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/xenxi/bio-cli/issues"
  },
  "homepage": "https://github.com/xenxi/bio-cli#readme",
  "devDependencies": {
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "boxen": "^7.0.1"
  }
}
